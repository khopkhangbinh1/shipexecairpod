      </Fields>
      <Query>
        <DataSourceName>KSPPSOFFICE</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select a.serial_number,
       a.carton_no,
       a.pallet_no,
       a.cdt,
       a.wc,
       a.location_no
  from ppsuser.t_sn_status a
 where a.serial_number in
       (select b.serial_number
          from sajet.g_sn_status b
         where b.pallet_no = :insn )
union
select a.serial_number,
       a.carton_no,
       a.pallet_no,
       a.cdt,
       a.wc,
       a.location_no
  from ppsuser.t_sn_status a
 where a.serial_number in (select b.serial_number
                             from sajet.g_sn_status b
                            where b.carton_no = :insn 
                              and b.carton_no &lt;&gt; 'N/A')
union
select a.serial_number,
       a.carton_no,
       a.pallet_no,
       a.cdt,
       a.wc,
       a.location_no
  from ppsuser.t_sn_status a
 where a.serial_number in
       (select b.serial_number
          from sajet.g_sn_status b
         where b.customer_sn = :insn 
           and b.customer_sn &lt;&gt; 'N/A')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name=":insn">
            <Value>=Parameters!insn.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="insn">
      <DataType>String</DataType>
      <Prompt>insn</Prompt>
