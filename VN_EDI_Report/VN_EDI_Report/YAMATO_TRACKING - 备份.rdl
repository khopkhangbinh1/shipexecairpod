      </Fields>
      <Query>
        <DataSourceName>PPS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT A.HAWB,A.TRACKING_NO FROM T_SN_STATUS A WHERE A.SHIPMENT_ID IN 
(
SELECT B.SHIPMENT_ID FROM T_SHIPMENT_INFO B WHERE B.CARRIER_CODE='XYMT' AND TRUNC(B.CLOSE_TIME) = TRUNC(:CloseDay)
)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name=":CloseDay">
            <Value>=Parameters!CloseDay.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
