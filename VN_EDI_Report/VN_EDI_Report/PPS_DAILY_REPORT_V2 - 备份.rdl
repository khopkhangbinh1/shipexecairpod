        </Field>
      </Fields>
      <Query>
        <DataSourceName>KSPPSOFFICE</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select aa.custmodel,
       aa.region,
       aa.SHIPPING_PLAN,
       aa.PPS_DONE,
       round((aa.PPS_DONE / aa.SHIPPING_PLAN) * 100, 2) || '%' "PPS_DONE_Percentage",
       aa.SHIPPING_PLAN - aa.PPS_DONE "PPS_TO_GO",
       aa.PGI_DONE,
       round((aa.PGI_DONE / aa.SHIPPING_PLAN) * 100, 2) || '%' "PGI_DONE_Percentage",
       aa.SHIPPING_PLAN - aa.PGI_DONE "PGI_TO_GO"
  from (select c.custmodel,
               a.region,
               sum(b.assign_qty) as "SHIPPING_PLAN",
               sum(decode(d.weight, null, 0, '', 0,0, 0, b.assign_qty)) "PPS_DONE",
               sum(decode(a.status, 'UF', b.assign_qty, 0)) "PGI_DONE"
          from ppsuser.t_shipment_info a
          join ppsuser.t_pallet_order b
            on a.shipment_id = b.shipment_id
          join pptest.oms_partmapping c
            on b.ictpn = c.part
          join ppsuser.t_shipment_pallet d
            on d.pallet_no = b.pallet_no
         where a.shipping_time &gt; = :startday
           and a.shipping_time &lt; :endday
           and a.status not in('CP','WS','IN','SF')
         group by c.custmodel, a.region
        UNION
        select c.custmodel,
               'TOTAL' region,
               sum(b.assign_qty) as "SHIPPING_PLAN",
               sum(decode(d.weight, null, 0, '', 0,0, 0, b.assign_qty)) "PPS_DONE",
               sum(decode(a.status, 'UF', b.assign_qty, 0)) "PGI_DONE"
          from ppsuser.t_shipment_info a
          join ppsuser.t_pallet_order b
            on a.shipment_id = b.shipment_id
          join pptest.oms_partmapping c
            on b.ictpn = c.part
          join ppsuser.t_shipment_pallet d
            on d.pallet_no = b.pallet_no
         where a.shipping_time &gt;= :startday
           and a.shipping_time &lt; :endday
           and a.status not in('CP','WS','IN','SF')
         group by c.custmodel) aa
 order by aa.custmodel asc, aa.region asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name=":startday">
            <Value>=Parameters!startday.Value</Value>
          </QueryParameter>
          <QueryParameter Name=":endday">
            <Value>=Parameters!endday.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
