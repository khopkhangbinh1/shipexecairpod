        </Field>
        <Field Name="PGI_DONE_Percentage">
          <DataField>PGI_DONE_Percentage</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PGI_TO_GO">
          <DataField>PGI_TO_GO</DataField>
