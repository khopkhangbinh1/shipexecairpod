        <Field Name="SHIPMENT_ID">
          <DataField>SHIPMENT_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PPS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT C.HAWB,A.HAWB AS SAWB,A.SHIPMENT_id FROM T_SHIPMENT_INFO A INNER JOIN T_SHIPMENT_SAWB B ON A.SHIPMENT_ID=B.SAWB_SHIPMENT_ID
INNER JOIN T_SHIPMENT_INFO C ON B.SHIPMENT_ID=C.SHIPMENT_ID
 WHERE  A.SAWBFLAG='Y'
 
 AND A.STATUS='UF'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
