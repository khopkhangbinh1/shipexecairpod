  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PPS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":ware">
            <Value>=Parameters!ware.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NowStock.PART_NO,NowStock.CartonQty,NowStock.Qty,NowStock.QHCarton,NowStock.QHQty,
nvl(DoCarton,0) AS DoCarton,nvl(DoQty,0) AS DoQty  FROM 
(select *
          from (select part_no,
                       sum(cartonqty) as cartonqty,
                       sum(qty) as qty,
                       sum(qhcartonqty) as qhcarton,
                       sum(qhqty) as qhqty
                  from ppsuser.t_location a
                 where a.warehouse_id = :ware
                 group by part_no
                having sum(qty) &gt; 0
                 order by part_no)
        
        union
        select *
          from (select distinct part_no,
                                0       as cartonqty,
                                0       as qty,
                                0       as qhcarton,
                                0       as qhqty
                  from ppsuser.t_location_log
                 where idt &gt; trunc(sysdate)
                   and warehouse_id = :ware
                   and part_no not in
                       (select part_no
                          from ppsuser.t_location
                         where part_no is not null))

)NowStock 
LEFT JOIN 

  (SELECT TSS.PART_NO,COUNT(DISTINCT tss.CARTON_NO) AS DoCarton,COUNT(tss.SERIAL_NUMBER) AS DoQty FROM PPSUSER.T_SN_STATUS tss where tss.WC IN('W1','W2','W3','W4')
GROUP BY TSS.PART_NO)PPSdo ON NowStock.PART_NO=PPSdo.PART_NO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
