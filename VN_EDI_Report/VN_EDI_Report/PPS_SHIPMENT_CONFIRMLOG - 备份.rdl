      <Query>
        <DataSourceName>PPS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select  a.shipment_id , 
       d.region , 
    d.shipping_time , 
    b.pallet_no , 
        e.ictpn , 
    e.carton_qty , 
    e.qty , 
    b.car_no , 
    decode(b.isload, 1, 'Y', 'N') , 
    c.whconfirm_name , 
    c.whconfirm_time , 
        c.securityconfirm_name , 
    c.securityconfirm_time 
from      ppsuser.t_shipment_pallet a 
    join pptest.oms_load_car b on a.shipment_id = b.shipment_id and 
          a.pallet_no = b.pallet_no and (b.active = 0 or b.active is null) 
    join ppsuser.t_shipment_pallet_part e on a.pallet_no = e.pallet_no 
    join ppsuser.t_shipment_info d on a.shipment_id = d.shipment_id 
    left join ppsuser.t_truck_confirm_log c on a.shipment_id = c.shipment_id and 
          a.pallet_no = c.pallet_no and c.car_no = b.car_no
where a.cdt &gt;= to_date(:startdaytime ,'yyyy-mm-dd')
and a.cdt &lt;= to_date(:enddaytime  ,'yyyy-mm-dd')
and upper(b.car_no) 
      = case 
    when trim(:carno) = '' then upper(b.car_no) 
    when trim(:carno) is null then upper(b.car_no) 
    else upper(:carno) 
    end
and upper(d.region) 
      = case 
    when trim(:vregion) = '' then upper(d.region) 
    when trim(:vregion) is null then upper(d.region)
    else upper(:vregion) 
    end    
order by d.region asc , d.shipping_time desc, b.pallet_no asc, e.ictpn asc</CommandText>
